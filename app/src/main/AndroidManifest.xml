            android:name=".activity.Screen_three"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_four"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_five"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_six"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_seven"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_eight"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_nine"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_ten"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.Screen_eleven"
            android:screenOrientation="landscape" />

        <service
            android:name=".BackgroundMusicService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>